<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.sewavespa" >

    <uses-permission android:name="android.permission.MANAGE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/icon_vespa"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@drawable/icon_vespa"
        android:supportsRtl="true"
        android:theme="@style/Theme.SewaVespa"
        android:usesCleartextTraffic="false"
        tools:replace="android:allowBackup"
        tools:targetApi="31" >
        <activity
            android:name=".prosespesanan"
            android:exported="false" />
        <activity
            android:name=".homecustomer"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MidtransActivity"
            android:exported="false" />
        <activity
            android:name=".NotaActivity"
            android:exported="false" />
        <activity
            android:name=".KonfirmasiActivity"
            android:exported="false" />
        <activity
            android:name=".riwayatpesanan"
            android:exported="false" />
        <activity
            android:name=".SewaActivity"
            android:exported="false" />
        <activity
            android:name=".auth.register.RegisterActivity"
            android:exported="false" />
        <activity
            android:name=".auth.login.LoginActivity"
            android:exported="false" />
        <activity
            android:name=".DetailActivity"
            android:exported="false" />
        <activity
            android:name=".SplashActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.SewaVespa.NoActionBar" />
    </application>

</manifest>